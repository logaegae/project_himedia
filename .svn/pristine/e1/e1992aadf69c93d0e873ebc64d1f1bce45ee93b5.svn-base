.franchise{
    word-break: keep-all;
    b{
        color:@content-color;
        font-weight: @title-weight;
    }

    .franchise-basebox-wrapper{
        width:100%;
        padding:1px;
        background-color: @base-color;


        .franchise-basebox-icons{
            .basebox-icon-box{
                width:120px;

                .basebox-contents{
                    display:block;
                }
            }
        }

        .franchise-basebox-icons{
            margin-top:35px;


            .basebox-icon-box{

                vertical-align: top;
                text-align: center;
                .inlineblock();

                &>span:first-child{
                    width:100px;
                    height:67px;
                    .inlineblock();

                }
                img{
                    .inlineblock();
                }
                &:first-child+span.basebox-icon-box{
                    width:110px;
                }
                &:first-child+.basebox-icon-box+span.basebox-icon-box{
                    width:125px;
                }
                &:first-child+.basebox-icon-box+.basebox-icon-box+.basebox-icon-box+span.basebox-icon-box{
                    width:130px;
                }
                .basebox-contents{
                    font-weight: @gnb-weight;
                    color:@content-color;
                }
            }
        }
    }
    .processInfo-wrapper{
        .title-m{
            margin:40px 0 30px;
        }
        .process-box{
            width:100%;
            height:640px;
            padding:70px 0;
            background: url('../img/site_img/bg_franchise.jpg') no-repeat center center;
            background-size: cover;

            &>div{
                width:1024px;
                margin:0 auto;

                .roundbox{
                    width:140px;
                    height:140px;
                    border: 5px @unit-sub-color solid;
                    display:table;
                    position:relative;
                    .radius(50%);

                    &.pull-left{
                        margin-left: 94px;
                        margin-bottom:100px;
                    }
                    &.pull-right{
                        margin-right: 94px;
                        margin-bottom:70px;
                    }
                    &>div{
                        display:table-cell;
                        vertical-align: middle;
                        text-align:center;

                        .roundbox-title{
                            font-size:14px;
                            font-weight: @gnb-weight;
                            color:@unit-sub-color;
                            display:block;
                        }
                        .roundbox-content{
                            font-size:@menu-size;
                            font-weight: @gnb-weight;
                            color:@white-color;
                            display:block;
                        }
                    }
                    &:first-child::after,&:first-child+li::after,&:first-child+li+li::after{
                        content:"";
                        .inlineblock();
                        width:32px;
                        height:32px;
                        background: url('../img/icon/img_arrowRed.png') no-repeat center center;
                        position:absolute;
                        right:-66px;
                        top:50%;
                        transform:translateY(-50%);

                    }
                    &:first-child+li+li+li::after{
                        content:"";
                        .inlineblock();
                        width:32px;
                        height:32px;
                        background: url('../img/icon/img_arrowRed.png') no-repeat center center;
                        position:absolute;
                        left:50%;
                        bottom:-66px;
                        transform:translateX(-50%) rotate(90deg);
                    }
                    &:first-child+li+li+li+li::after,&:first-child+li+li+li+li+li::after,&:first-child+li+li+li+li+li+li::after{
                        content:"";
                        .inlineblock();
                        width:32px;
                        height:32px;
                        background: url('../img/icon/img_arrowRed.png') no-repeat center center;
                        position:absolute;
                        top:50%;
                        left:-66px;
                        transform:translateY(-50%) rotate(180deg);
                    }
                }
                .btn-red{
                    width:220px;
                    height:55px;
                    line-height: 55px;
                    padding: 0 22px;
                    background: @main-color;
                    color:@white-color;
                    font-size:@menu-size + 1;
                    .inlineblock();

                    .btn_content{
                        margin-right: 20px;
                    }
                    i.icon{
                        .inlineblock();
                    }
                }
            }
        }
    }
    .modalPopup{
        .agree-box{
            height:130px;
            padding:10px 10px;
            overflow-y:scroll;
            color:@content-color;
            border:1px @sub-color-second solid;
        }
        input[type="checkbox"]#agreeCheck{
            height:auto;
            width:auto;
            margin-left:0;
        }
        .btns{
            vertical-align: top;
        }
    }
    .btn-red{
        background-color: @main-color;
        color:@white-color;
    }
    .btn-black{
        background-color: @sub-color;
        color:@white-color;
    }
    .btn-sm{
        width:100px;
        font-size:@menu-size;
        font-weight: @subject-weight;
        .inlineblock();
        .radius(0px);
        border:none;
    }
    #agreeCheck+label{
        font-size: 14px;
        text-indent: 0;
        position: static;
    }
}
