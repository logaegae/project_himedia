.service{
    .contents{
        .board{
            width: 750px;
            height: 700px;
            background-color: @sub-color;
            margin: 0 auto;
            position: relative;
            margin-bottom: 70px;
                
                span{
                    font-size: @menu-size;
                    color:white;
                    display: inline-block;
                    line-height: 40px;
                    padding-left:10px;
                }
            
            .board-contents{
                width: 728px;
                height: 625px;
                background-color: white;
                margin: 0 auto;
                padding: 10px 0 10px 10px;
                
                textarea{
                    width: 100%;
                    height: 100%;
                    border: 0;
                    overflow-y: scroll;
                }
            }
        }
    }
}