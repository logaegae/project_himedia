        .btnsr{
            margin-top: 60px;
            margin-bottom:40px;
            input{
                padding:12px 28px;
                margin-right:10px;
                border:none;
                color:@white-color;
                font-weight: @gnb-weight;
            }
            input[type="submit"]{
                background-color: @main-color;
            }
            input[type="reset"]{
                background-color: @sub-color;
            }
        }

        .star_wrap{
            line-height: 20px;
            .inlineblock();
            width:100%;
            span{
                .inlineblock();
                width:13px;
                height:12px;
                background:url('/img/site_img/staroff.png') no-repeat;
                text-indent:-9999px;
                font-size:0;
            }
            span.on{
                background:url('/img/site_img/staron.png') no-repeat;
            }
        }



}
