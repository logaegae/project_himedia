.reviewWrite{
    .inputtable_write{
        table.input_table{
            margin:0 auto 20px;
        }
        p.find_id{
            padding-left:10px;
            text-align: center;
            font-size:@summery-size;
            color:@content-color;
            font-weight: @subject-weight;
            margin-bottom:30px;
            span{
                vertical-align: middle;
            }
            .idicon{
                display:inline-block;
                *display:inline;
                *zoom:1;
                *display:inline;
                *zoom:1;
                width:25px;
                height:27px;
                background:url('/img/site_img/id_bg.jpg');
                margin-right:12px
            }
            // .red_bar{
            //     width:31px;
            //     height:2px;
            //     display:block;
            //     background:@main-color;
            //     margin:20px auto;
            // }

        }
        .btn_wrap{
            text-align: center;
            color:@white-color;
            input{
                width:80px;
                height:40px;
                background:@main-color;
                border:none;
                font-size:16px;
                font-weight: @gnb-weight;
            }
        }

        .pw_fieldset{
            margin-top:70px;
            p.find_pw{
                padding-left:10px;
                text-align: center;
                font-size:@summery-size;
                color:@content-color;
                font-weight: @subject-weight;
                margin-bottom:30px;
                span{
                    vertical-align: middle;
                }
                .pwicon{
                    display:inline-block;
                    *display:inline;
                    *zoom:1;
                    width:23px;
                    height:30px;
                    background:url('/img/site_img/pw_bg.jpg');
                    margin-right:12px
                }
                // .red_bar{
                //     width:31px;
                //     height:2px;
                //     display:block;
                //     background:@main-color;
                //     margin:20px auto;
                // }
            }
            .btn_wrap{
                margin-bottom:70px;
            }
        }
    }
