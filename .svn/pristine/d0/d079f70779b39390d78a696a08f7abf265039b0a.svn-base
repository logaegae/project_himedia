.footer-modal{
    overflow-y: auto;
    .modal-body{
        padding:40px 70px;
    }
    .agree-box{
        height:130px;
        padding:10px 10px;
        overflow-y:scroll;
        color:@content-color;
        border:1px @sub-color-second solid;
    }
    .checkbox{
        margin-bottom:0;
        text-align: right;
        color:@white-color;

    }
    input[type="checkbox"]#agreeCheck{
        height:auto;
        width:auto;
        margin-left:0;
    }
    .btns{
        vertical-align: top;
        margin-top:40px;
    }
    #agreeCheck+label{
        font-size: 14px;
        text-indent: 0;
        position: static;
    }
}


.footerEmail{
    .modalPopup{
        .modal-title{
            line-height: 40px;
            .title-box{
                font-size:20px;
            }
        }
        .modal-body{
            font-size:14px;
            padding: 40px 30px;
        }
    }
}
