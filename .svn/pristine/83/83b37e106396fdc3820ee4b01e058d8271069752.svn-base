.field-webPublishing{
    .lnb-course{
        margin-bottom:70px;
        .inner{
            li{
                a.on{
                    background-color: #3b4852;
                    color:@white-color;
                }
            }
        }
    }
    .sm-rect-btns-wrapper{
        margin-bottom:70px;

        .sm-rect-btns{
            width:246px;
            height:50px;
        }
        &.last{
            margin-right:0;
        }
    }
    .type_wrap{

        >div{
            display:none;
        }
        >div.on{
            display:block;
        }
        .type1{
            padding-top:70px;
            width:1024px;
            margin:0 auto;
            .type1_top{
                border-top:1px solid #8f8e97;
                border-bottom:1px solid #8f8e97;
                width: 750px;
                margin:0 auto 85px;
                padding:50px 0;
                text-align: center;
                .img-box{
                    margin-bottom:40px;
                    display: block;
                }
                .title{
                    color:@subject-color;
                    font-size:@summery-size;
                    span{
                        font-weight: @title-weight+100;
                    }
                }
                .explain{
                    font-size:50px;
                    line-height: 70px;
                    font-weight: @title-weight+100;
                    color:@content-color;
                    span{
                        color:@main-color;
                        padding:0 45px;
                    }
                }
            }
            .whatis{
                text-align: center;
                width:750px;
                margin:0 auto;
                padding-bottom:70px;
                border-bottom:1px solid #7e7d87;
                .title{
                    color:@content-color;
                    font-size:@summery-size;
                    font-weight: @title-weight+100;
                    line-height: 115px;
                    .red_bar{
                        height:23px;
                        width:4px;
                        background: @main-color;
                        .inlineblock();
                        vertical-align: middle;
                        margin-right:20px;
                    }
                }
                .cont{
                    font-size:@menu-size;
                    text-align: left;
                    color:@content-color;
                }
                >div{
                    margin-top:70px;
                    p{
                        font-size:@summery-size;
                        font-weight: @title-weight+100;
                        span{
                            display:block;
                            width:30px;
                            height:2px;
                            background:@main-color;
                            margin:20px auto;
                        }
                    }
                    span{
                        margin-left:20px;
                    }
                }
            }
            .portfolio{
                padding-top:70px;
                >p{
                    text-align: center;
                    font-size:@summery-size;
                    font-weight: @title-weight+100;
                    span{
                        display:block;
                        width:30px;
                        height:2px;
                        background:@main-color;
                        margin:20px auto 70px;
                    }
                }
                .link{
                    background-color: @sub-color;
                    margin-bottom:70px;
                    width:1024px;
                    margin:0 auto 70px;

                    a{
                        color:@white-color;
                        font-size:16px;
                        text-align: center;
                        width:100%;
                        height: 100%;
                        .inlineblock();
                        line-height: 65px;
                    }
                    span.icon{
                        margin-left: 18px;
                    }
                }
            }
        }
        .type2{
            .open_class{
                background:@base-color;
                .wrap{
                    margin:0 auto;
                    width:1024px;
                    padding:70px 0;
                    position:relative;
                    overflow: hidden;
                    .tab{
                        li{
                            width:68.2px !important;
                        }
                    }
                    .title{
                        font-size:@subject-size;
                        text-align: center;
                        font-weight: @title-weight+100;
                        .red_bar{
                            display:block;
                            margin:20px auto;
                            width:30px;
                            height:2px;
                            background:@main-color;
                        }
                    }
                    .cont{
                        font-size:@menu-size;
                        line-height:@summery-size;
                        text-align: center;
                        font-weight: @gnb-weight;
                        span{
                            display:block;
                            margin:0 auto;
                            color:@subject-color;
                        }
                    }
                    .class_btn_wrap{
                        width:1024px;
                        margin:40px auto 0;
                        text-align: center;
                        font-size:0;
                        >a{
                            .inlineblock();
                            padding:0 20px;
                            height:40px;
                            line-height: 40px;
                            border:1px solid @sub-color;
                            font-size:16px;
                            font-weight: @gnb-weight;
                            background:@white-color;
                            color:@sub-color;
                            margin:0 10px 10px 0;
                            >span{
                                .inlineblock();
                                font-size:13px;
                                width:50px;
                                height:25px;
                                background:@main-color;
                                color:@white-color;
                                line-height: 23px;
                                .radius(20px);
                                vertical-align: top;
                                margin-top: 8px;
                            }
                            &&:hover{
                                background:@sub-color;
                                color:@white-color;
                            }
                        }
                    }
                }
            }
            .field_Wrap{
                width:1024px;
                margin:0 auto;
                position: relative;
                height:70px;
                .show_box{
                    position:relative;
                    top:-44px;
                    right:0;
                    overflow: hidden;
                    .round{
                        .radius(50%);
                        width:88px;
                        height:88px;
                        background:@sub-color;
                        color:@white-color;
                        text-align: center;
                        padding:19px 0;
                        line-height: 25px;
                        float:right;
                        .ncs{
                            font-size:@summery-size;
                            font-weight: @gnb-weight;
                        }
                        >span{
                            font-size:@menu-size;
                        }
                    }
                    p{
                        text-align: right;
                        clear:both;
                        display:block;
                        padding-top:15px;
                        font-size:@menu-size;
                        position:relative;
                        right:-1000px;
                        >span{
                            font-weight:@title-weight+100;
                        }
                        a{
                            .inlineblock();
                            width:211px;
                            height:40px;
                            line-height: 40px;
                            color:@white-color;
                            background: @sub-color;
                            text-align: center;
                            margin-top:15px;
                            >span{
                                margin-left:25px;
                            }
                            &&:hover{
                                background:@main-color;
                                >span{
                                    background-position: -80px 0;
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
