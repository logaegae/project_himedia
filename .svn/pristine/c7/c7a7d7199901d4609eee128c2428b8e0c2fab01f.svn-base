.onlineCounsel{
    .onlineCounsel_bg{
        background:@base-color;
        width:100%;
    }
    .page1_tab{
        p{
            width:1024px;
            margin:0 auto;
            font-size:@summery-size;
            margin-bottom:20px;
            font-weight:@gnb-weight;
            line-height: 18px;
        }
        .tab{
            margin-bottom:30px;
            ul {overflow:hidden;}
            li {
                float: left;
                margin-left:5px;
                &&:first-child{
                    margin-left:0
                }
                button {
                    display:block;
                    width:100%;
                    height:50px;
                    color:@subject-color;
                    font-weight:@gnb-weight;
                    text-align: center;
                    vertical-align: middle;
                    background:@white-color;
                    padding: 0 20px;
                }
                &:hover {
                    button {
                        background:@sub-color-first;
                        color:@white-color;
                    }
                }
            }
            li.on {
                button {
                    color:@white-color;
                    background:@main-color;
                }
            }
        }
    }
    .select_processwrap{
        padding-bottom:40px;
    }
    .btn_wrap{
        text-align: center;
        padding:30px 0;
        a{
            display:inline-block;
            *display:inline;
            *zoom:1;
            width:100px;
            height:50px;
            line-height:50px;
            color:@white-color;
            background:#252e33;
        }
        span.arrow{
            margin-left:15px;
        }
    }
}
