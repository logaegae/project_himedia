/* Reset */
@import url('https://fonts.googleapis.com/css?family=Anton');
@charset "utf-8";
/* Reset */
html,
body {
  width: 100%;
  height: 100%;
  position: relative;
}
html {
  overflow-y: scroll;
  font-size: 14px;
}
body {
  position: relative;
  min-width: 1200px;
  font-family: 'Noto Sans KR', 'Droid Serif', sans-serif;
}
body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
form,
fieldset,
p,
button {
  margin: 0;
  padding: 0;
}
dl,
ul,
ol,
li {
  list-style: none;
}
textarea {
  resize: none;
}
a {
  text-decoration: none;
}
a:hover {
  text-decoration: none;
}
a:focus {
  text-decoration: none;
  outline: none;
}
a:-webkit-any-link {
  text-decoration: none;
}
input,
button {
  vertical-align: middle;
}
::-moz-selection {
  background: #e62e2e;
  color: #ffffff;
}
::selection {
  background: #e62e2e;
  color: #ffffff;
}
button {
  border: 0;
  background: none;
}
.glyphicon-home {
  color: #e62e2e;
}
.icon {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  font-size: initial;
  font-size: 0;
  text-indent: -999999%;
  vertical-align: middle;
  background: url('/img/icon/icon_sprite.png') no-repeat;
}
.icon.arrow {
  width: 12px;
  height: 20px;
}
.icon.plus {
  width: 19px;
  height: 19px;
}
.icon.arrow-slide {
  width: 24px;
  height: 38px;
}
.icon.check {
  width: 25px;
  height: 21px;
}
.icon.list {
  width: 19px;
  height: 17px;
}
.icon.delete {
  width: 21px;
  height: 25px;
}
.icon.arrow.red {
  background-position: 0 0;
}
.icon.arrow.small {
  width: 10px;
  height: 17px;
  background-position: -80px -45px;
}
.icon.arrow.navy {
  background-position: -40px 0;
}
.icon.arrow.white {
  background-position: -80px 0;
}
.icon.arrow.grey {
  background-position: -120px 0;
}
.icon.arrowDown.red {
  width: 20px;
  height: 12px;
  background-position: 0 -50px;
}
.icon.arrowUp.red {
  width: 20px;
  height: 12px;
  background-position: -40px -50px;
}
.icon.plus.red {
  background-position: 0 -100px;
}
.icon.plus.white {
  background-position: -40px -100px;
}
.icon.close-x.red {
  width: 20px;
  height: 20px;
  background-position: 0 -150px;
}
.icon.arrow-slide.left {
  background-position: 0 -200px;
}
.icon.arrow-slide.right {
  background-position: -80px -200px;
}
.icon.arrow-slide.left-black {
  background-position: -120px -200px;
}
.icon.arrow-slide.right-black {
  background-position: -200px -200px;
}
.icon.check.red {
  background-position: 0 -250px;
}
.icon.check.white {
  background-position: -40px -250px;
}
.icon.list.red {
  background-position: 0 -300px;
}
.icon.delete.red {
  background-position: 0 -350px;
}
.icon.delete.red2 {
  background-position: -40px -350px;
}
.icon.new {
  width: 18px;
  height: 17px;
  background-position: -80px -350px;
}
.icon.red.arrow_left {
  background-position: -158px 0;
  width: 12px;
  height: 20px;
}
.wrapper {
  position: relative;
}
select {
  -webkit-appearance: none;
  /* 네이티브 외형 감추기 */

  -moz-appearance: none;
  appearance: none;
  background: url('/img/site_img/select_arrow.jpg') no-repeat 95% 50%;
  /* 화살표 모양의 이미지 */

  border-radius: 0;
  border: 1px solid #252e33;
  height: 36px;
  width: 163px;
  padding: 0 7px;
  font-size: 13px;
  color: #333333;
  margin-right: 9px;
}
/* IE 10, 11의 네이티브 화살표 숨기기 */
select::-ms-expand {
  display: none;
}
