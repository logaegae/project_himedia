.modalPopup {
  border: 2px #252e33 solid;
  display: none;
  z-index: 1000;
  position: fixed;
  left: 50%;
  top: 50%;
  -moz-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #FFF;
}
.modalPopup .modal-title {
  background-color: #252e33;
  color: #ffffff;
  padding: 10px 32px;
}
.modalPopup .modal-title .title-box {
  font-size: 30px;
  font-weight: 500;
  border-bottom: 2px #252e33 solid;
  float: left;
}
.modalPopup .modal-title .close-box {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  font-size: initial;
  height: 40px;
  line-height: 40px;
}
.modalPopup .modal-title .close-box a {
  color: transparent;
}
.modalPopup .modal-title .close-box i {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  font-size: initial;
  margin-left: 20px;
}
.modalPopup .modal-body {
  font-size: 15px;
  font-weight: 400;
  color: #666666;
  line-height: 20px;
  padding: 70px 70px;
}
.modalPopup .modal-body strong {
  font-weight: 600;
  font-size: 18px;
  color: #333333;
}
.modalPopup .modal-body .content-title {
  position: relative;
  margin-bottom: 10px;
}
.modalPopup .modal-body .content-title:after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  font-size: initial;
  width: 30px;
  height: 2px;
  background-color: #e62e2e;
}
.modalPopup .modal-body .content-title .sub-title {
  font-weight: 500;
  font-size: 18px;
  color: #333333;
  padding-bottom: 10px;
}
.modalPopup .modal-body .content-title .name {
  font-weight: 500;
  font-size: 30px;
  color: #333333;
  padding-bottom: 30px;
}
