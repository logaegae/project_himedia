<script>
    // 상단탭
    $('.sm-rect-btns').click(function () {
        $(this).addClass('active').siblings().removeClass('active');
        var curri_index = $(this).index();
        $('.type>div').eq(curri_index).addClass('on').siblings().removeClass('on')
    });
    // 중간컨텐츠탭
    $('.inner_tab li').click(function () {
        $(this).addClass('on').siblings().removeClass('on');
        var inner_index = $(this).index();
        $('.inner_tab_cont>div').eq(inner_index).addClass('on').siblings().removeClass('on')
    });
    //하단 동그라미
    $('.round').mouseover(function () {
        $('.field_Wrap p').animate({
            'right': 0
        });
        $('.field_Wrap').css({
            'height': '226px'
        });
    });
    $('.show_box').mouseleave(function () {
        $('.field_Wrap p').animate({
            'right': '-1000px'
        }, function () {
            $('.field_Wrap').css({
                'height': '70px'
            });
        })
    });
</script> {% endblock %}