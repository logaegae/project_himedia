.modalPopup{
    border: 2px @sub-color solid;
    display:none;
    z-index: 100;
    position:fixed;
    left:50%;
    top:50%;
    transform:translate(-50%,-50%);
    background-color: #FFF;

    &.showOn{
        .inlineblock();
    }

    .modal-title{
        background-color: @sub-color;
        color:@white-color;
        padding:10px 32px;

        .title-box{
            font-size:@subject-size + 5;
            font-weight:@gnb-weight;
            border-bottom: 2px @sub-color solid;
            float:left
        }

        .close-box{
            .inlineblock();
            height:40px;
            line-height:40px;

            a{
                color:@white-color;
            }
            i{
                .inlineblock();
                margin-left:20px;
            }
        }
    }
    .modal-body{
        font-size:@menu-size;
        font-weight:@subject-weight;
        color:@subject-color;
        line-height:20px;
        padding:70px 70px;

        strong{
            font-weight:@title-weight;
            font-size:@summery-size - 2;
            color:@content-color;
        }
        .content-title{
            position:relative;
            margin-bottom:10px;

            &::after{
                content:"";
                position:absolute;
                bottom:0;
                left:0;
                .inlineblock();
                width:30px;
                height:2px;
                background-color: @main-color;
            }

            .sub-title{
                font-weight:@gnb-weight;
                font-size:@summery-size - 2;
                color:@content-color;
                padding-bottom:10px;
            }

            .name{
                font-weight: @gnb-weight;
                font-size:@subject-size + 5;
                color:@content-color;
                padding-bottom:30px;
            }
        }
    }
}
.modalBlind{
    position:fixed;
    left:0;
    right:0;
    top:0;
    bottom:0;
    background: rgba(0,0,0,0.3);
    z-index: 10;
}
