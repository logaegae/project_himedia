/* jshint indent: 2 */

module.exports = function(sequelize, DataTypes) {
  return sequelize.define('BranchTeacher', {
    branchCode: {
      type: DataTypes.STRING,
      allowNull: false
    },
    teacherSeq: {
      type: DataTypes.INTEGER(11),
      allowNull: false
    }
  }, {
    tableName: 'BranchTeacher'
  });
};
