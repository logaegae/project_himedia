.jobSeeker {
    .sm-rect-btns-wrapper {
        margin-bottom: 57px;
        .sm-rect-btns {
            width: 32%;
            font-size: 16px;
        }
    }
    .type {
        >div {
            display: none;
            .inner_tab {
                margin: 70px 0;
                ul {
                    font-size: 0;
                    vertical-align: top;
                    li {
                        .inlineblock();
                        width: 140px;
                        margin-left: 15px;
                        button {
                            width: 100%;
                            height: 25px;
                            color: @content-sub-color;
                            font-weight: @gnb-weight;
                            font-size: @menu-size + 3;
                            span {
                                display: block;
                                width: 100%;
                                height: 3px;
                                margin-top: 5px;
                                background: @content-sub-color;
                            }
                        }
                    }
                    li:hover {
                        button {
                            span {
                                background: @main-color;
                            }
                        }
                    }
                    li.on {
                        button {
                            color: @content-color;
                            span {
                                background: @main-color;
                            }
                        }
                    }
                    li:first-child {
                        margin-left: 0;
                    }
                }
            }
        }
        >div.on {
            display: block;
        }
        .type-a {
            text-align: center;
            .type_top {
                width: 1024px;
                margin: 0 auto;
                h2 {
                    font-size: @subject-size;
                    line-height: 46px;
                    font-weight: @title-weight+100;
                    color: @content-color;
                }
                .red_bar {
                    display: block;
                    margin: 13px auto 20px;
                    width: 30px;
                    height: 2px;
                    background: @main-color;
                }
                p {
                    font-size: @menu-size;
                    line-height: @summery-size;
                    color: @subject-color;
                    font-weight: @gnb-weight;
                    span {
                        color: @content-color;
                    }
                }
                .img_banner {
                    margin: 70px 0;
                }
                .inner_tab {
                    margin-bottom: 70px;
                    ul {
                        font-size: 0;
                        vertical-align: top;
