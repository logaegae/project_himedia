
.inlineblock(){
    display: inline-block;
    *display:inline;
    *zoom:1;
    font-size:initial;
}


.radius(@r:5px){
    -webkit-border-radius:@r;
    -moz-border-radius:@r;
    border-radius:@r;
}

.box-sizing(){
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.inlineblockIm(){
    display: inline-block !important;
    *display:inline !important;
    *zoom:1;
    font-size:initial;
}
