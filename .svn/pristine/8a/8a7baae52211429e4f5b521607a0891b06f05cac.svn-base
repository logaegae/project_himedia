.input_fromwrap{


    .select_wrap{
        text-align: center;
        margin: 0 0 70px;
        font-size:0;
        legend{
            position: absolute;
            font-size: 0;
            text-indent: -9999px;
            border: none;
        }

        label{
            font-size:0;
            text-indent: -9999px;
            position:absolute;
        }

        select{
            -webkit-appearance: none;  /* 네이티브 외형 감추기 */
            -moz-appearance: none;
            appearance: none;
            background: url('/img/site_img/select_arrow.jpg') no-repeat 95% 50%;  /* 화살표 모양의 이미지 */
            border-radius: 0;
            border:1px solid @sub-color;
            height:36px;
            width:163px;
            padding: 0 7px;
            font-size:13px;
            color:@content-color;
            margin-right:9px;
        }

        /* IE 10, 11의 네이티브 화살표 숨기기 */
        select::-ms-expand {
          display: none;
        }
    }

    .input_tablewrap{

        legend{
            font-size: 0;
            text-indent: -9999px;
            border: none;
            position:absolute;
        }

        table.input_table{
            width:750px;
            margin: 0 auto 40px;
            font-size:13px;
            border:2px solid #7e7d87;
            border-width: 2px 0;

            caption{
                font-size:0;
                text-indent: -9999px;
                margin:0;
                height:0;
                padding: 0;
            }
            tr{

                th{
                    font-size:13px;
                    color:@content-color;
                    border-top:1px solid #bababf;
                }
                td{
                    padding:18px 0;
                    border-top:1px solid #bababf;
                    .input_wrap{
                        position:relative;
                        display:inline-block;
                        *display:inline;
                        *zoom:1;
                        label{
                            font-size:0;
                            text-indent: -9999px;
                            position:absolute;
                        }
                        input{
                            border:1px solid #8c9093;
                            height:26px;
                            width:100%;
                            padding:0 9px;
                            &&:focus{
                                ~button.input_del{
                                    display:block
                                }
                            }

                        }
                        input.hasDatepicker{
                            width:75%;
                        }
                        textarea{
                            border:1px solid #8c9093;
                            width:100%;
                            padding:9px;
                            min-height:95px;
                            overflow-y: scroll;
                            resize:none;
                        }
                        .input_del{
                            background: url('/img/site_img/input_del.jpg') no-repeat 50% 50%;
                            z-index:2;
                            width: 21px;
                            height: 100%;
                            font-size: 0;
                            text-indent: -9999px;
                            position: absolute;
                            right:9px;
                            top: 0;
                            display:none;
                            &&:focus,&&:hover{
                                display:block;
                            }
                        }
                    }
                    select{
                        -webkit-appearance: none;  /* 네이티브 외형 감추기 */
                        -moz-appearance: none;
                        appearance: none;
                        background: url('/img/site_img/select_arrow.jpg') no-repeat 95% 50%;  /* 화살표 모양의 이미지 */
                        border-radius: 0;
                        border:1px solid @sub-color;
                        height:26px;
                        width:200px;
                        padding: 0 7px;
                        font-size:13px;
                        color:@content-color;
                        margin-right:9px;
                        vertical-align: middle;
                    }
                    /* IE 10, 11의 네이티브 화살표 숨기기 */
                    select::-ms-expand {
                      display: none;
                    }
                }
                &&:first-child{
                    th,td{
                        border-top:0;
                    }
                }
            }
        }
    }
}


.view_table{
