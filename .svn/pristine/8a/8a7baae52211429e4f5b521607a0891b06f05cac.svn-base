// footer
.footer {
    height:182px;
    clear:both;
    .top {
        margin:34px auto 0;
        width:1200px;
        height:100px;
        > div {
            .inlineblock();
            font-size:@content-size - 2;
            font-weight:@gnb-weight;
            color:@content-color;
            line-height:18px;
            vertical-align:top;
        }
        .logo {
            padding:0 46px 0 119px;
        }
        .info-link-box {
            padding:0 45px;
            height:70px;
            border-right:1px #d8d8db solid;
            border-left:1px #d8d8db solid;
            .left {
                float: left;
            }
            .right {
                margin-left:142px;
            }
            > div {
                // width:110px;
                width:220px;
                text-align: left;
                .float_left{
                    float:left;
                    width:50%;
                    text-align: left;
                }
                .float_right{
                    float:right;
                    width: 50%;
                    text-align: right;
                }
                a {
                   display:block;
                   color:@content-color;
                   .txt-red {
                       color:@main-color;
                   }
                }
            }
        }
        .address {
            padding-left:40px;
            text-align:left;
            address {
                margin-bottom:0;
            }
            .copyright {color:@subject-color;}
        }
    }
    .sns-link {
        text-align: center;
        .inner {
            .inlineblock();
            border-top: 1px @sub-color solid;
            a {
                .inlineblock();
                padding-top:10px;
            }
        }
    }
}

.lnb-course {
    height:200px;
    overflow:hidden;
    text-align:center;
    background-color:@base-color;
    position: relative;
    .inner {
        z-index: 5;
        .inlineblock();
        width:1024px;
        position:relative;
        ul {
            // width:100%;
        }
        li {
            float: left;
            width:12.5%;
            text-align:center;
            position:relative;
            background:@base-color;
            .hover-red:hover {background-color:@main-color;}
            .hover-navy:hover {background-color:@sub-color-first;}
            .hover-grey:hover {background-color:@sub-color-second;}
            .hover-green:hover {background-color:@unit-main-color;}
            .hover-red.on {
                background-color:@main-color;
                >.txt-box{color:@white-color;}
            }
            .hover-navy.on {
                background-color:@sub-color-first;
                >.txt-box{color:@white-color;}
            }
            .hover-grey.on {
                background-color:@sub-color-second;
                >.txt-box{color:@white-color;}
            }
            .hover-green.on {
                background-color:@unit-main-color;
                >.txt-box{color:@white-color;}
            }
            a {
                .inlineblock();
                padding-top:32px;
                width:100%;
                height:200px;
                color:@title-color;
                &:hover {color:@white-color}
                .img-box {
                    display:block;
                    height:105px;
                    width:105px;
                    margin:0 auto;
                    img{
                        width:100%;
                        height:100%;
                    }
                }
                .txt-box {
                    display:block;
                    font-size:@menu-size;
                    font-weight:@gnb-weight;
                }
            }
            button{
                .inlineblock();
                padding-top:0;
                width:100%;
                height:200px;
                color:@title-color;
                &:hover {color:@white-color}
                .img-box {
                    display:block;
                    height:105px;
                    width:105px;
                    margin:0 auto;
                    img{
                        width:100%;
                        height:100%;
                    }
                }
                .txt-box {
                    display:block;
                    font-size:@menu-size;
                    font-weight:@gnb-weight;
                }
            }
            .location{
                width:100%;
                margin:0 auto;
                height: 40px;
                position:absolute;
                text-align: center;
                display:none;
            }
        }
    }

    .hide_cont.on{
        display: block;
        padding:40px 0;
    }
    .hide_cont{
        position:relative;
        top:0;
        display:none;
        width:100%;
        height:auto;
        border-top:2px solid @main-color;
        background:@white-color;
        font-size:0;
        .wrap{
            width: 980px;
            margin:0 auto;
            height:auto;
            .box{
                width:50%;
                .inlineblock();
                text-align: left;
                vertical-align: top;
                p{
                    font-size:@summery-size;
                    margin-bottom:20px;
                    font-weight: @gnb-weight;
                }
                >span{
                    margin:20px 15px;
                }
                ul{
                    fon-size:0;
                    li{
                        .inlineblock();
                        width:49.5%;
                        font-size:13px;
                        a{
                            color:@content-color;
                            line-height: 20px;
                        }
                    }
                }
            }
            .box1{
                padding-right:16px;
            }
            .box2{
                padding-left:16px;
            }
        }
    }
    .hide_cont.on{
        display:block;
    }
}


.sub-navi {
    margin:0 auto;
    width:1024px;
    height:50px;
    line-height:50px;
    font-size:0;
    text-align: right;
    > * {
        margin-left:9px;
        font-size:@content-size - 1;
        color:@sub-color-second;
    }
    .this-page {
        color:@title-color;
        font-weight:@subject-weight;
    }
}

.title-l {
    margin-bottom:70px;
    text-align:center;
    position:relative;
    .bg {
            font-family: @font-anton;
            font-size:200px;
            line-height:172px;
            font-weight:500;
            letter-spacing:3px;
            color:@base-color;
        }
    .text-box {
        position:absolute;
        top:50%;
        left:0;
        margin-top:-30px;
        width:100%;
        > div {
            margin:0 auto;
        }
        h1 {
            height:45px;
            line-height:45px;
            font-size:@title-size;
            font-weight:@title-weight;
            &:before {
                .inlineblock();
                content: "";
                margin-right:20px;
                width:10px;
                height:45px;
                vertical-align: top;
                background-color:@main-color;
            }
        }
        p {
            margin-top:14px;
            font-size:@menu-size;
            color:@sub-color-second;
        }
    }
}

.title-m {
    margin:70px 0;
    text-align:center;
    h2 {
        position:relative;
        font-size:@subject-size;
        font-weight:@title-weight;
        &:after {
            position:absolute;
            top:49px;
            left:50%;
            .inlineblock();
            content: "";
            margin-left:-15.5px;
            width:31px;
            height:3px;
            vertical-align: top;
            background-color:@main-color;
        }
    }
    p {
        margin-top:48px;
        font-size:@menu-size;
        line-height:20px;
        color:@sub-color-second;
    }
}

