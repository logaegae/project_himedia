.bottom_checkbox{
    width:750px;
    margin:0 auto;
    padding:0 10px 10px;
    background:#252e33;
    p{
        font-size:@menu-size;
        color:@white-color;
        font-weight: @subject-weight;
        line-height: 44px;
    }
    div.cont{
        padding:10px;
        background:@white-color;
        overflow-y: scroll;
        height:125px;
        font-size:13px;
    }
    div.agree_wrap{
        text-align: right;
        margin-top:9px;
        label{
            font-size:13px;
            color:@white-color;
            margin-bottom:0;
        }
        input[type="checkbox"]{
            margin:0
        }
    }
}

.step_wrap{
    text-align: center;
    margin-bottom:30px;
    padding-top:70px;
    ol{
        li{
            display:inline-block;
            *display:inline;
            *zoom:1;
            font-size:0;
            text-align: center;
            margin-left:60px;
            position:relative;
            &&:after{
                content: "";
                display: inline-block;
                background:url('/img/icon/icon_sprite.png') no-repeat;
                background-position: -120px 0;
                width:12px;
                height:20px;
                position:absolute;
                top: 18px;
                left: -36px;
            }
            &&:first-child{
                margin-left:0;

                &&:after{
                    background:none;
                    width:0;
                    height:0
                }
            }
            span{
                display:block;
                font-size:@summery-size;
                color:@content-sub-color;
                &&.num{
                    font-weight: @title-weight;
                }
                &&.text{
                    font-weight: @subject-weight;
                    font-size:19px;
                    letter-spacing: -1px;
                }
            }

            &&.on{
                span.num{
                    color:@main-color;
                }
                span.text{
                    color:@content-color;
                }
            }
        }
    }

}
.pre{
    white-space:pre-wrap;
    word-break:keep-all;
}
.br-lf-none{
