        min-width: 65px;
        button {
            width:100%;
            height:61px;
            border:0;
            border-top:2px solid @sub-color;
            background-color:@white-color;
        }
    }
    li:hover button {
        font-weight:@gnb-weight;
        background-color:@base-color;
    }
    li.on button{
        font-weight:@title-weight;
        color:@white-color;
        background-color:@main-color;
    }
}

.tab-customer {
    ul {overflow:hidden;}
    li {
        float: left;
        margin-left:23px;
        &:first-child {margin-left:0;}
        a {
            display:table-cell;
            width:120px;
            height:120px;
            color:@sub-color-first;
            line-height:18px;
            font-weight:@gnb-weight;
            vertical-align: middle;
            text-align:center;
            .radius(100px);
            // behavior: url(PIE.htc);
            border: 3px solid @sub-color-first;
        }
        &:hover {
            a {
                color:@sub-color-second;
                border: 3px solid @sub-color-second;
            }
        }
    }
    li.on {
        a {
            color:@main-color;
            border: 3px solid @main-color;
        }
    }
}

.tab-customer-campus {
    ul {overflow:hidden;}
    li {
        float: left;
        button {
            display:block;
            width:100%;
            height:50px;
            color:@subject-color;
            font-weight:@gnb-weight;
            text-align: center;
            vertical-align: middle;
            background:@white-color;
        }
        &:hover {
            button {color:@main-color;}
        }
    }
    li.on {
        button {
            color:@white-color;
            background:@main-color;
        }
    }
}

.pagination {
    width:100%;
    margin:70px 0;
    text-align:center;
    > a {
        .inlineblock();
        width:18px;
        height:16px;
        font-size:0;
        text-indent:-9999999%;
        vertical-align:middle;
        background:url('/img/icon/icon_pagination.png') no-repeat;
    }
    .first {
        margin-right:23px;
        background-position:0 0;
        &:hover {background-position:0 -46px;}
    }
    .prev {
        width:9px;
        background-position:-47px 0;
        &:hover {background-position:-47px -46px;}
    }
    .next {
        width:9px;
        background-position:-86px 0;
        &:hover {background-position:-86px -46px;}
    }
    .last {
        margin-left:23px;
        background-position:-124px 0;
        &:hover {background-position:-124px -46px;}
    }
    span {
        .inlineblock();
        margin:0 17px;
        a {
            .inlineblock();
            width:29px;
            height:29px;
            line-height:25px;
            color:@content-color;
            border:1px #8b9093 solid;
            &:hover {
                color:@main-color;
                border:1px @main-color solid;
            }
        }
        a.on {
            color:@white-color;
            border:1px @main-color solid;
            background-color:@main-color;
        }
    }
}



.btn-seemore {
    margin:70px auto;
    max-width:1024px;
    button {
        width:100%;
        height:65px;
        line-height:65px;
        color:@white-color;
        font-size:@menu-size + 1;
        font-weight:@subject-weight;
        background-color:@sub-color;
        .icon {margin-left:20px;}
        &:hover {
            background-color:@main-color;
            .icon {background-position: -40px -100px;}
        }
    }
}
.showOn{
    .inlineblockIm();
}
.modalPopup{
    border: 2px @sub-color solid;
    display:none;
    z-index: 1000;
    position:fixed;
    left:50%;
    top:50%;
    -moz-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    background-color: #FFF;


    .modal-title{
        background-color: @sub-color;
        color:@white-color;
        padding:10px 32px;

        .title-box{
            font-size:@subject-size + 5;
            font-weight:@gnb-weight;
            border-bottom: 2px @sub-color solid;
            float:left
        }

        .close-box{
            .inlineblock();
            height:40px;
            line-height:40px;

            a{
                color:transparent;
            }
            i{
                .inlineblock();
                margin-left:20px;
            }
        }
    }
    .modal-body{
        font-size:@menu-size;
        font-weight:@subject-weight;
        color:@subject-color;
        line-height:20px;
        padding:70px 70px;

        strong{
            font-weight:@title-weight;
            font-size:@summery-size - 2;
            color:@content-color;
        }
        .content-title{
            position:relative;
            margin-bottom:10px;

            &:after{
                content:"";
                position:absolute;
                bottom:0;
                left:0;
                .inlineblock();
                width:30px;
                height:2px;
                background-color: @main-color;
            }

            .sub-title{
                font-weight:@gnb-weight;
                font-size:@summery-size - 2;
                color:@content-color;
                padding-bottom:10px;
            }

            .name{
                font-weight: @gnb-weight;
                font-size:@subject-size + 5;
                color:@content-color;
                padding-bottom:30px;
            }
        }
    }
}
.modalBlind{
    position:fixed;
    left:0;
    right:0;
    top:0;
    bottom:0;
    background: rgba(0,0,0,0.3);
    z-index: 10;
}
.modalBlind_trans{
    position:fixed;
    left:0;
    right:0;
    top:0;
    bottom:0;
    background: rgba(0,0,0,0.0);
    z-index: 10;
}

