//selsect_box
select{
    -webkit-appearance: none;  /* 네이티브 외형 감추기 */
    -moz-appearance: none;
    appearance: none;
    background: url('/img/site_img/select_arrow.jpg') no-repeat 95% 50%;  /* 화살표 모양의 이미지 */
    border-radius: 0;
    border:1px solid @sub-color;
    height:36px;
    width:163px;
    padding: 0 7px;
    font-size:13px;
    color:@content-color;
    margin-right:9px;
}

/* IE 10, 11의 네이티브 화살표 숨기기 */
select::-ms-expand {
  display: none;
}

