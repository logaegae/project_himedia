.findIdpw{
    .inputtable_wrap{
        padding-top:40px;
        table.input_table{
            margin:0 auto 20px;
        }
        p.find_id{
            padding-left:10px;
            text-align: center;
            font-size:@summery-size;
            color:@content-color;
            font-weight: @subject-weight;
            margin-bottom:30px;
            span{
                vertical-align: middle;
            }
            .idicon{
                display:inline-block;
                *display:inline;
                *zoom:1;
                *display:inline;
                *zoom:1;
                width:25px;
                height:27px;
                background:url('/img/site_img/id_bg.jpg');
                margin-right:12px
            }

        }
        .btn_wrap{
            text-align: center;
            color:@white-color;
            input{
                width:80px;
                height:40px;
                background:@main-color;
                border:none;
                font-size:16px;
                font-weight: @gnb-weight;
            }
        }

        .pw_fieldset{
            margin-top:70px;
            p.find_pw{
                padding-left:10px;
                text-align: center;
                font-size:@summery-size;
                color:@content-color;
                font-weight: @subject-weight;
                margin-bottom:30px;
                span{
                    vertical-align: middle;
                }
                .pwicon{
                    display:inline-block;
                    *display:inline;
                    *zoom:1;
                    width:23px;
                    height:30px;
                    background:url('/img/site_img/pw_bg.jpg');
                    margin-right:12px
                }
                // .red_bar{
                //     width:31px;
                //     height:2px;
                //     display:block;
                //     background:@main-color;
                //     margin:20px auto;
                // }
            }
            .btn_wrap{
                margin-bottom:70px;
            }
        }
    }
    .result_on{
        background:@base-color;
        margin: 20px 0;
        padding:70px;
        text-align: center;
        letter-spacing: -0.5px;
        p.id_title{
            font-size:@menu-size;
            color:@content-color;
            font-weight: @subject-weight;
        }
        p.id{
            color:@main-color;
            font-weight: @gnb-weight;
            font-size:@subject-size;
        }
        p.date{
            padding:10px 0;
            color:@subject-color;
            font-size:13px;
        }
        p.cont{
            color:@subject-color;
            font-size:13px;
            span{display: block}
        }
        p.pw_title{
            font-weight: @gnb-weight;
            font-size:@summery-size;
            letter-spacing: 0;
            padding-bottom:10px;
            span{
                color:@main-color;
            }
        }
        p.pw_cont{
            span{
                font-weight:700;
                letter-spacing: 0;
            }

        }
        a{
            width:127px;
            height:40px;
            display:inline-block;
            *display:inline;
            *zoom:1;
            color:@white-color;
            background:@sub-color;
            line-height:40px;
            margin-top:40px;
            span{
                margin-left:30px
            }
            &&:hover{
                background:@main-color;
                span{
                    background-position: -80px 0;
                }
            }
        }
    }
}
