{% extends "./blocks/layout.html" %}
{% block content %}
<div class="onlineCounsel">
        <!-- 본문 -->
        <div class="contents">
            <!-- 서브 네비 -->
            <div class="sub-navi">
                <a href="#">HOME</a><span>&gt;</span>
                <span>고객센터</span><span>&gt;</span>
                <span class="this-page">온라인상담</span>
            </div>
            <!-- // 서브 네비 -->

            <!-- 대제목 -->
            <div class="title-l">
                <div class="bg">Customer</div>
                <div class="text-box">
                    <h1>온라인 상담</h1>
                    <p>17초 작성완료! 전문가 온라인 상담 신청</p>
                </div>
            </div>
            <!-- // 대제목 -->

            <!-- 탭부분1 -->
            <div class="tab tab-customer">
    		    <ul>
    		        <li><a href="notice.html">공지사항/<br>이벤트</a></li>
    		        <li><a href="askCost.html">수강료<br>조회</a></li>
    		        <li><a href="askTime.html">시간표<br>조회</a></li>
    		        <li class="on"><a href="#">온라인<br>상담</a></li>
    		        <li><a href="onlinePay.html">온라인<br>결제</a></li>
    		        <li><a href="comFaq.html">FAQ</a></li>
    		        <li><a href="qna.html">Q&amp;A</a></li>
    		    </ul>
    		</div>
            <!-- //탭부분 -->

            <!-- 컨텐츠 -->
            <div class="onlineCounsel_bg">
                <div class="step_wrap">
                    <ol>
                        <li class="on">
                            <span class="num">STEP 1.</span>
                            <span class="text">과정선택</span>
                        </li>
                        <li>
                            <span class="num">STEP 2.</span>
                            <span class="text">연락처 작성</span>
                        </li>
                        <li>
                            <span class="num">STEP 3.</span>
                            <span class="text">상담신청</span>
                        </li>
                    </ol>
                </div>

                <div class="select_campuswrap page1_tab">
                    <p>캠퍼스 선택</p>
                    <div class="tab select_campus">
                        <ul>
                            <li class="on"><button type="button">강남</button></li>
                            <li><button type="button">종로1</button></li>
                            <li><button type="button">종로2</button></li>
                            <li><button type="button">신촌</button></li>
                            <li><button type="button">구로</button></li>
                            <li><button type="button">노원</button></li>
                            <li><button type="button">천호</button></li>
                            <li><button type="button">안양</button></li>
                            <li><button type="button">안산</button></li>
                            <li><button type="button">성남</button></li>
                            <li><button type="button">수원</button></li>
                            <li><button type="button">인천</button></li>
                            <li><button type="button">전주</button></li>
                            <li><button type="button">구리</button></li>
                        </ul>
                    </div>
                </div>
                <div class="select_areawrap page1_tab">
                    <p>유형 선택</p>
                    <div class="tab select_area">
                        <ul>
                            <li class="on"><button type="button">학생 및 일반과정</button></li>
                            <li><button type="button">직장인 국비지원과정</button></li>
                            <li><button type="button">실업자 국비지원 과정</button></li>
                        </ul>
                    </div>
                </div>
                <div class="select_groupwrap page1_tab">
                    <p>분야 선택</p>
                    <div class="tab select_group">
                        <ul>
                            <li class="on"><button type="button">웹 디자인</button></li>
                            <li><button type="button">웹 퍼블리싱</button></li>
                            <li><button type="button">웹 개발</button></li>
                            <li><button type="button">전산회계/세무</button></li>
                            <li><button type="button">편집/출판 디자인</button></li>
                            <li><button type="button">영상</button></li>
                            <li><button type="button">캐드</button></li>
                            <li><button type="button">인테리어</button></li>
                            <li><button type="button">OA</button></li>
                        </ul>
                    </div>
                </div>
                <div class="select_processwrap page1_tab">
                    <p>과정 선택</p>
                    <div class="tab select_process">
                        <ul>
                            <li class="on"><button type="button">웹 디자인+퍼블리셔 종합반</button></li>
                            <li><button type="button">웹 표준 코딩 기본</button></li>
                            <li><button type="button">HTML 5.0/CSS3</button></li>
                            <li><button type="button">Javascript &amp; jQuery</button></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="btn_wrap">
                <a class="chage_color_btn" href="onlineCounselStep2.html">다음<span class="icon arrow red">오른쪽 화살표</span></a>
            </div>
            <!-- //컨텐츠 -->
        </div>
        <!-- // 본문 -->
</div>
{% endblock %}
