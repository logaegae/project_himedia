.joinFinish{
    .step_wrap{
        padding-top:0;
    }
    .finishcont{
        border:1px solid #7e7d87;
        border-width: 2px 0 2px 0;
        width:750px;
        margin: 0 auto;
        text-align: center;
        padding:70px 0;
        line-height:23px;
        >img{
            margin-bottom:40px;
        }
        .name{
            font-size:@summery-size;
            line-height:30px;
            padding-bottom:5px;
            span{
                font-weight:@title-weight;
            }
        }
        .title{
            font-weight: @gnb-weight;
        }
        .title,.sub_title{
            font-size:@menu-size;
        }
    }
    .btns_wrap{
        text-align: center;
        padding:70px 0 40px;
        font-size:0;
        a{
            .inlineblock();
            padding:0 21px;
            line-height:40px;
            height:40px;
            color:@white-color;
            background:@sub-color;
            font-size:16px;
        }
        a.main{
            background:@main-color;
            margin-right:10px;
        }
    }
}
