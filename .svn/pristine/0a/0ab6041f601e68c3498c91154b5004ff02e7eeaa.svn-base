.footerFranchise{
    overflow-y: auto;
    // height:80vh;
        .modal-body{
            padding:40px 70px;
        }
        .agree-box{
            height:130px;
            padding:10px 10px;
            overflow-y:scroll;
            color:@content-color;
            border:1px @sub-color-second solid;
        }
        .checkbox{
            margin-bottom:0;
            text-align: right;
            color:@white-color;
        }
        input[type="checkbox"]#agreeCheck{
            height:auto;
            width:auto;
            margin-left:0;
        }
        .btns{
            vertical-align: top;
            margin-top:40px;
        }
        .btn-red{
            background-color: @main-color;
            color:@white-color;
            margin-right:10px;
        }
        .btn-black{
            background-color: @sub-color;
            color:@white-color;
        }
        .btn-sm{
            width:100px;
            font-size:@menu-size;
            font-weight: @subject-weight;
            .inlineblock();
            .radius(0px);
            border:none;
        }
        #agreeCheck+label{
            font-size: 14px;
            text-indent: 0;
            position: static;
        }
}



@media (max-width: 1023px){
    .footerFranchise{
        .modal-body{
            padding:40px 3%;
        }
    }
    .moadl_foot.showOn{
        .footerFranchise{
            height:82vh;
        }
    }
}
